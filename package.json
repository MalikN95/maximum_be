{
  "name": "maximum_back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:production": "yarn start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js,json,yalm,yml}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js,json,yalm,yml}\" --fix",
    "upgrade:all": "yarn-upgrade-all",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/conf/data_source.ts",
    "typeorm:generate": "yarn ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/conf/data_source.ts migration:generate",
    "typeorm:run": "yarn typeorm migration:run",
    "typeorm:drop": "npm run typeorm schema:drop ",
    "prepare": "husky install",
    "docker:exec": "docker exec -it bamboo_be_dev",
    "docker:shell": "yarn docker:exec sh",
    "docker:yarn": "yarn docker:exec sh -c 'yarn && touch package.json'",
    "docker:if:host:yarn": "if command -v docker &>/dev/null 2>&1; then docker top bamboo_be_dev > /dev/null 2>&1 && yarn run docker:yarn; fi; exit 0",
    "postinstall": "yarn docker:if:host:yarn",
    "docker:stop:all": "docker stop `docker ps -a -q`"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.8.0",
    "pg-mem": "^2.6.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.10",
    "zod": "^3.21.4",
    "aws-sdk": "^2.1560.0"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.19.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.18.53",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-typesafe": "^0.5.2",
    "husky": "^8.0.1",
    "jest": "28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
